        //alert(nofile);
      //  alert('4')
        var tbcol = "#tbcolcht_" + tbnum;
        var conlist = "#tbresult_" + tbnum;
        var resultmap = "#map_" + tbnum;
        var resultmapcol = "#mapcol_" + tbnum;
        var formaterr = "#tbforerror_" + tbnum;
        var forval = $(formaterr).text();
        //alert(forval);
        ///alert(conlist);
        var configstring = $(conlist).text();

        var mapcount = $(resultmap).text();
        var nomapcol = $(resultmapcol).text();

        var tbcollst = $(tbcol).text();
     //   alert('5')
      //  alert(forval)
        if (forval == "no") {
           // alert('6')
            $("#config_choose").hide();
            $("#config_success").hide();
            $("#config_error").show();
            $("#config_warning").hide();
        }
        else {
           // alert('7')
            //alert(mapcount);
            if (mapcount == '0') {
                //沒有一個欄位匹配所以也不用更新
                //alert('1111 no doata');
                var tbcolarr = new Array();
                tbcolarr = tbcollst.split(',');
                // alert(tbcolarr.length);
                //alert('nomapp ' + tbcolarr.length.toString());
                var newtbcol = "";
                for (var x = 0; x < tbcolarr.length; x++) {
                    newtbcol += "「" + tbcolarr[x] + "」" + "、";
                }
              //  alert('8')
                newtbcol = newtbcol.substring(0, newtbcol.length - 1);
                $(resultmap).html(tbcolarr.length.toString());
                $(resultmapcol).html(newtbcol);
                $("#config_choose").hide();
                $("#config_warning").show();
                //$(resultmap).html(tbcolarr.length);
            }
            else {
                var pid = $('#project_id').text();

             //   alert('9')
                $.ajax({
                    type: "get",
                    url: "/api/WebAPI/UpdateColSetting",
                    contentType: "application/json",
                    async: false,
                    data:
                    {
                        pid: pid, configstring: configstring
                    },
                    success: function (response) {
                        if (response == "1") {
                            //success
                            //location.reload();
                            if (nomapcol == "") {
                                $("#config_choose").hide();
                                $("#config_success").show();
                            }
                            else {
                                //   alert('11');
                                $(resultmapcol).html(nomapcol);

                                $("#config_choose").hide();
                                $("#config_warning").show();
                            }
                        }
                        //else if (response == "-1") {

                        //}
                        //else if (response == "-2") {
                        //    //no pid
                        //}
                        //else if (response == "-10") {
                        //    // string error
                        //}
                        else {
                            //another error
                            var tbDatacount = $('#savecolerror').text();
                            //alert(tbDatacount);
                            $("#config_choose").hide();
                            $("#config_success").hide();
                            $("#config_error").show();
                            $("#config_warning").hide();
                        }
                    },
                    error: function (response) {
                        // alert('1111儲存欄位資料失敗');
                        var tbDatacount = $('#savecolerror').text();
                        //alert(tbDatacount);
                        $("#config_choose").hide();
                        $("#config_success").hide();
                        $("#config_error").show();
                        $("#config_warning").hide();
                    }
                });
            }
        }
    }

    function reloadconfig() {
        //alert('1111');
        location.reload();
    }
</script>