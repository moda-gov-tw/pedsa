                            // var colval = strs[2];
                            col = strcol.split(',');
                            //col_value = colval.split(',');
                            var strcol_value = strs[2];
                            // alert(strcol_value);
                            col_value = strcol_value.split(',');
                            for (var i = 0; i < col.length; i++) {
                                var selectname = "#" + tbname + "_" + col[i] + " option[value=\'" + col_value[i] + "\']";
                                $(selectname).attr('selected', 'selected');
